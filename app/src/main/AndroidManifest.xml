<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.motoel.motocharqueadas">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Principal"
            android:label="MOTO CHARQUEADAS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".programacao_completa"
            android:label="@string/title_activity_programacao_completa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Frame_principal" />
        <activity
            android:name=".tab_programacao"
            android:label="@string/title_activity_tab_programacao"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>